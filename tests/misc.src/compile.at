AT_SETUP([“N” in the VARYING clause])

AT_DATA([prog.cbl], [
       IDENTIFICATION              DIVISION.
       PROGRAM-ID.                 prog.
       DATA                        DIVISION.
       WORKING-STORAGE             SECTION.
       01 VAL PIC N(5) VALUE N"あいうえお".
       PROCEDURE                   DIVISION.
           DISPLAY "Hello".
           STOP RUN.
])
AT_CHECK([ocesql prog.cbl prog.cob > /dev/null], [0])

AT_CLEANUP

